import java.util.Date;
String startTime = vars.get(&quot;startTime&quot;);
log.info(&quot;====================&quot;+startTime+&quot;=====================&quot;);
SimpleDateFormat format = new SimpleDateFormat(&quot;yyyy-MM-dd HH:mm:ss&quot;);
//the date need to change before run test
Date date = format.parse(startTime);
long waitTime = date.getTime()-System.currentTimeMillis();
log.info(&quot;------wait ------&quot;+waitTime);
Thread.sleep(waitTime&gt;0?waitTime:1);</stringProp>
            </BeanShellTimer>
