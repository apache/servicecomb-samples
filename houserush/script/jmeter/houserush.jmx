                  <stringProp name="filename"></stringProp>
                  <stringProp name="script">import java.util.ArrayList;
import java.util.List;
import com.alibaba.fastjson.JSON;
import com.alibaba.fastjson.JSONArray;
import com.alibaba.fastjson.JSONObject;

String response = prev.getResponseDataAsString();
log.info(response);
JSONObject res = JSON.parseObject(response);
JSONArray arrs = res.getJSONArray(&quot;houseOrders&quot;);
List list = new ArrayList();
for(int i=0;i&lt;arrs.size();i++){
      JSONObject obj = arrs.getJSONObject(i);
      if(obj.get(&quot;state&quot;).equals(&quot;new&quot;)){
        list.add(obj);
      }
}
if(list.size()==0){
	vars.put(&quot;houseOrderId&quot;,&quot;&quot;);
}else{
	int selectIndex = (int)(Math.random()*(list.size()));
	JSONObject obj = list.get(selectIndex);
	log.info(&quot;----get houseOrderId------&quot;+obj.get(&quot;id&quot;));
	vars.put(&quot;houseOrderId&quot;,&quot;&quot;+obj.get(&quot;id&quot;));
}
</stringProp>
